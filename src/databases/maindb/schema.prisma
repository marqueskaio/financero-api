generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DB_URL")
}

model UsersModel {
  id          Int     @id @default(autoincrement())
  name        String
  email       String
  role        String
  password    String
  accessToken String?

  @@map("users")
}
